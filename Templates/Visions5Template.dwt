<script>
function myFunction() {
		var offset = $('nav').offset();
var height = $('nav').height();
var width = $('nav').width();
document.getElementById('demo').innerHTML = height;
document.getElementById('demo').innerHTML = width;
var top = offset.top + height + "px";
var right = offset.left + width + "px";

$('main').css( {
    'position': 'absolute',
    'right': right,
    'top': top
});
